{
    "name": "pabloveintimill/frekr",
    "description": "API wrapper of Frekr",
    "keywords": ["php", "api", "frekr"],
    "type": "library",
    "minimum-stability": "dev",    
    "require": {
        "guzzlehttp/guzzle": "^6.3@dev"
    },
    "require-dev": {
        "php": ">=7.1",
        "symplify/easy-coding-standard": "^5.5@dev",
        "phpunit/phpunit": "^8",
        "paulthebaud/phpunit-generator": "2.0",
        "symfony/var-dumper": "^4.3@dev",
        "phpmd/phpmd": "dev-master"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Pablo Veintimilla Vargas",
            "email": "pabloveintimilla@gmail.com"
       }
    ],
    "autoload": {
        "psr-4": {
            "PabloVeintimilla\\Frekr\\": "src/"
        },
        "classmap": ["src"]
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "test-win": "vendor\\bin\\phpunit.bat",
        "gentest-win": "vendor\\bin\\phpunitgen.bat"
    }    
}
